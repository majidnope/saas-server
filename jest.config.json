{
  "clearMocks": true,  
  "testEnvironment": "node",
  "preset": "ts-jest",
  "verbose": true,
  "testPathIgnorePatterns": ["dist"],
  "collectCoverageFrom": ["src/**/*.ts"],
  "coverageThreshold": {
    "global": {
      "branches": 0,
      "functions": 0,
      "lines": 0,
      "statements": 0
    }
  },
  "moduleDirectories": ["node_modules", "src"]
}




